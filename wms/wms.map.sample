LAYER
  NAME "zp_relue"
  GROUP "zp"
  STATUS ON
  TYPE POLYGON
  CONNECTIONTYPE POSTGIS
  CONNECTION "host=geonatdbhost dbname=geonaturedb user=geonatuser password=monpassachanger"
  DATA "the_geom_local FROM florepatri.t_zprospection USING UNIQUE indexzp"
  FILTER "validation = 't' AND supprime != 't' "
  PROJECTION
    "init=epsg:MYLOCALSRID"
  END
  CLASS
    NAME "zp_relue"
    STYLE
      OUTLINECOLOR 100 45 130
      SIZE 2
    END
  END
  OPACITY 50
END

