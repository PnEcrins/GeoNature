MAP
  STATUS ON
  EXTENT 872150 1942230 943685 2030790 #(PNE à vérifier)
  SIZE 800 500
  #SHAPEPATH "/var/local"
  IMAGECOLOR 255 255 255
  SYMBOLSET "symbols.txt"
  FONTSET "fonts.txt"
  IMAGETYPE png

  PROJECTION
    "init=epsg:MYLOCALSRID"
  END

  CONFIG "MS_ERRORFILE" "/tmp/ms_error.txt"

  OUTPUTFORMAT
    NAME "png"
    DRIVER "AGG/PNG"
    #DRIVER "GD/PNG"
    MIMETYPE "image/png"
    IMAGEMODE RGBA
    TRANSPARENT ON
    EXTENSION "png"
    FORMATOPTION "INTERLACE=OFF"
  END

  LEGEND
    STATUS ON
    KEYSIZE 15 10
    IMAGECOLOR 255 255 255
    LABEL
      TYPE TRUETYPE
      SIZE 8
      COLOR 0 0 0
      FONT vera
    END
  END


  WEB
    METADATA
      wms_title "WMS GEONATURE"
      wms_onlinesource "http://%FQDN%/%PROJECT%/%VERSION%/wmsgeonature"
      "ows_enable_request" "*"
      wms_srs "EPSG:4326 EPSG:3857 EPSG:4171 EPSG:32622 EPSG:2154 EPSG:MYLOCALSRID"
  END
  IMAGEPATH "/tmp/"
  END

